theme_classic() +
theme(legend.position = "bottom")
plot2<- plot_model(m10,type = "slope",terms = "year") +
xlab(label = "Año")+
ylab(label = "Salario justo")+
ggtitle(label = "")+
facet_grid(labeller = labeller()) +theme_classic()
splot3<- plot_grid(x = list(plot1,plot2),margin = c(1,1,1,1),tags = c("",""))
